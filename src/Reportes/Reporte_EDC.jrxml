     venta.`Empleado` AS venta_Empleado,
     venta.`Cliente` AS venta_Cliente,
     sum(venta.`Monto`) AS venta_Monto
FROM
     `venta` venta
where  venta.`Fecha_De_Venta`=$P{fechaDeVenta}]]>
